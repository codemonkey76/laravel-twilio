{
  "name": "codemonkey76/laravel-twilio",
  "description": "Wrapper for twilio/sdk",
  "type": "library",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10",
    "illuminate/http": "^10",
    "twilio/sdk": "^7"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Shane Poppleton",
      "email": "shane@bjja.com.au"
    }
  ],
  "autoload": {
    "psr-4": {
      "Codemonkey76\\Twilio\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Codemonkey76\\Twilio\\TwilioServiceProvider"
      ],
      "aliases": {
        "Sms": "Codemonkey76\\Twilio\\Facades\\TwilioFacade"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
